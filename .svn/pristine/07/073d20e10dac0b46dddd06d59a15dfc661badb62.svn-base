    public function isCanDeclare($code)
    {
        $code = str_replace('-', '_', $code);
        $strFun = "canDeclare_".$code  ;
        if (!in_array($strFun, get_class_methods(self::class))) {
            throw new Exception('此渠道不能预览报关单' . $strFun);
        }
        return $this->$strFun($code);
    }
